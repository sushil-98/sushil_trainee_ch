<odoo>
	<data>
		
		<record id="agriexpert_form_view" model="ir.ui.view">
			<field name="name">agriexpert.detail.form</field>
			<field name="model">agriexpert.detail</field>
			<field name="arch" type="xml">
				<form string="Agri expert details">
					<sheet>
						<group>
							<field name="name"/>
							<field name="degree"/>
							<field name="license_no"/>
							<field name="specialist"/>
							<field name="description"/>
						</group>
					</sheet>
				</form>	
			</field>	
		</record>

		<record id="agriexpert_tree_view" model="ir.ui.view">
			<field name="name">agriexpert.detail.tree</field>
			<field name="model">agriexpert.detail</field>
			<field name="arch" type="xml">
				<tree string="Agri expert details tree view">
					<field name="name"/>
					<field name="degree"/>
					<field name="license_no"/>
					<field name="specialist"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

		<record id="agriexpert_detail_kanban" model="ir.ui.view">
            <field name="name">agriexpert.detail.kanban</field>
            <field name="model">agriexpert.detail</field>
            <field name="arch" type="xml">
                <kanban default_group_by="specialist">
                    <field name="color"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div
                                    t-attf-class="oe_kanban_color_{{kanban_getcolor(record.color.raw_value)}}
                                                  oe_kanban_global_click_edit oe_semantic_html_override
                                                  oe_kanban_card {{record.group_fancy==1 ? 'oe_kanban_card_fancy' : ''}}">
                                <div class="oe_dropdown_kanban">
                                    <!-- dropdown menu -->
                                    <div class="oe_dropdown_toggle">
                                        <i class="fa fa-bars fa-lg" title="Manage" aria-label="Manage"/>
                                        <ul class="oe_dropdown_menu">
                                            <li>
                                                <a type="delete">Delete</a>
                                            </li>
                                            <li>
                                                <ul class="oe_kanban_colorpicker"
                                                    data-field="color"/>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="oe_clear"></div>
                                </div>
                                <div t-attf-class="oe_kanban_content">
                                    <!-- title -->
                                    Nname:
                                    <field name="name"/>
                                    <br/>
                                    Degree:
                                    <field name="degree"/>
                                    <br/>
                                    License No:
                                    <field name="license_no"/>
                                    <br/>
                                    Specialist:
                                    <field name="specialist"/>
                                    <br/>
                                    description:
                                    <field name="description"/>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>
		<record id="agriexpert_detail_action" model="ir.actions.act_window">
			<field name="name">Agri Expert Details</field>
			<field name="res_model">agriexpert.detail</field>
			<field name="view_mode">tree,form,kanban</field>
		</record>

		<record id="farmer_detail_form_view" model="ir.ui.view">
			<field name="name">farmer.detail.form</field>
			<field name="model">farmer.detail</field>
			<field name="arch" type="xml">
				<form string="Farmer details">
					<sheet>
						<group>
							<field name="name"/>
							<field name="mobile_no"/>
							<field name="email"/>
							<field name="address"/>
						</group>
					</sheet>
				</form>	
			</field>	
		</record>

		<record id="farmer_detail_tree_view" model="ir.ui.view">
			<field name="name">farmer.detail.tree</field>
			<field name="model">farmer.detail</field>
			<field name="arch" type="xml">
				<tree string="Farmer details tree view">
					<field name="name"/>
					<field name="mobile_no"/>
					<field name="email"/>
					<field name="address"/>
				</tree>
			</field>
		</record>

		<record id="farmer_detail_action" model="ir.actions.act_window">
			<field name="name">Farmer Details</field>
			<field name="res_model">farmer.detail</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record id="crop_detail_form_view" model="ir.ui.view">
			<field name="name">crop.detail.form</field>
			<field name="model">crop.detail</field>
			<field name="arch" type="xml">
				<form string="Crop details">
					<sheet>
						<group>
							<field name="name"/>
							<field name="seed_name"/>
							<field name="seed_no"/>
						</group>
					</sheet>
				</form>	
			</field>	
		</record>

		<record id="crop_detail_tree_view" model="ir.ui.view">
			<field name="name">crop.detail.tree</field>
			<field name="model">crop.detail</field>
			<field name="arch" type="xml">
				<tree string="Crop details tree view">
					<field name="name"/>
					<field name="seed_name"/>
					<field name="seed_no"/>
				</tree>
			</field>
		</record>

		<record id="crop_detail_action" model="ir.actions.act_window">
			<field name="name">Crop Details</field>
			<field name="res_model">crop.detail</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record id="farmer_inquiry_form_view" model="ir.ui.view">
			<field name="name">farmer.inquiry.detail.form</field>
			<field name="model">farmer.inquiry.detail</field>
			<field name="arch" type="xml">
				<form string="Inquiry details form view">
					<sheet>
						<group>
							<field name="problem"/>
							<field name="crop_id"/>
							<field name="image"/>
							<field name="expert_id"/>
						</group>
					</sheet>
				</form>	
			</field>	
		</record>

		<record id="farmer_inquiry_tree_view" model="ir.ui.view">
			<field name="name">farmer.inquiry.detail.tree</field>
			<field name="model">farmer.inquiry.detail</field>
			<field name="arch" type="xml">
				<tree string="Inquiry details tree view">
					<field name="problem"/>
					<field name="crop_id"/>
					<field name="image"/>
					<field name="expert_id"/>
				</tree>
			</field>
		</record>

		<record id="farmer_inquiry_action" model="ir.actions.act_window">
			<field name="name">Inquiry Details</field>
			<field name="res_model">farmer.inquiry.detail</field>
			<field name="view_mode">tree,form</field>
		</record>


		<record id="expert_availability_form_view" model="ir.ui.view">
			<field name="name">agriexpert.availability.form</field>
			<field name="model">agriexpert.availability</field>
			<field name="arch" type="xml">
				<form string="create your schedule">
					<sheet>
						<group>
							<field name="day_from"/>
							<field name="time_from"/>
						</group>
						<group>
							<field name="day_to"/>
							<field name="time_to"/>
						</group>
					</sheet>
				</form>	
			</field>	
		</record>

		<record id="expert_availability_tree_view" model="ir.ui.view">
			<field name="name">agriexpert.availability.tree</field>
			<field name="model">agriexpert.availability</field>
			<field name="arch" type="xml">
				<tree string='tree view'>
					<field name="day_from"/>
					<field name="time_from"/>
					<field name="day_to"/>
					<field name="time_to"/>
				</tree>	
			</field>	
		</record>

		

		<record id="expert_availability_action" model="ir.actions.act_window">
			<field name="name">create your schedule</field>
			<field name="res_model">agriexpert.availability</field>
			<field name="view_mode">tree,form</field>
		</record>
		<record id="agriexpert_search_action" model="ir.actions.act_window">
			<field name="name">list of agriexpert</field>
			<field name="res_model">agriexpert.detail</field>
			<field name="view_mode">tree,form</field>
		</record>


		<menuitem id="main_menu" name="Agri App"/>

		<menuitem id="menuitem1" name="Agriexpert" action="agriexpert_detail_action" parent="main_menu"/>
		<menuitem id="menuitem2" name="Farmer" action="farmer_detail_action" parent="main_menu"/>
		<menuitem id="menuitem3" name="Inquiry" action="farmer_inquiry_action" parent="menuitem2" sequence="1"/>
		<menuitem id="menuitem4" name="Crop" action="crop_detail_action" parent="main_menu"/>
		<menuitem id="menuitem5" name="Search Agri Expert" action="agriexpert_search_action" parent="menuitem2" sequence="2"/>
		<menuitem id="menuitem7" name="Fill your details" action="agriexpert_detail_action" parent="menuitem1" sequence="1"/>
		<menuitem id="menuitem6" name="Availability/Schedule" action="expert_availability_action" parent="menuitem1" sequence="2"/>


		<record id="search_tree_view" model="ir.ui.view">
        	<field name="name">agriexpert.detail.search</field>
        	<field name="model">agriexpert.detail</field>
        	<field name="arch" type="xml">
            	<search>
            		<field name="name"/>
                	<field name="specialist"/>
            	</search>
        	</field>
    	</record>

	</data>
</odoo>